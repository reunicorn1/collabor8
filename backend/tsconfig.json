{
  "compilerOptions": {
	"module": "commonjs",
	"resolveJsonModule": true,
	"declaration": true,
	"removeComments": true,
	"emitDecoratorMetadata": true,
	"experimentalDecorators": true,
	"allowSyntheticDefaultImports": true,
	"target": "ES2021",
	"sourceMap": true,
	"outDir": "./dist",
	"baseUrl": "./",
	"incremental": true,
	"skipLibCheck": true,
	"strictNullChecks": false,
	"noImplicitAny": false,
	"strictBindCallApply": false,
	"forceConsistentCasingInFileNames": false,
	"noFallthroughCasesInSwitch": false,
	"paths": {
		"@admin/*": ["src/admin/*"],
		"@auth/*": ["src/auth/*"],
		"@users/*": ["src/users/*"],
		"@projects/*": ["src/projects/*"],
		"@project-shares/*": ["src/project-shares/*"],
		"@environment-mongo/*": ["src/environment-mongo/*"],
		"@project-mongo/*": ["src/project-mongo/*"],
		"@project-shares-mongo/*": ["src/project-shares-mongo/*"],
		"@directory-mongo/*": ["src/directory-mongo/*"],
		"@file-mongo/*": ["src/file-mongo/*"],
		"@utils/*": ["src/utils/*"],
		"@filters/*": ["src/filters/*"],
		"@constants": ["src/constants"],
		"@config/*": ["src/config/*"],
		"@hocuspocus/*": ["src/hocuspocus/*"],
		"@events/*": ["src/events/*"],
		"@mail/*": ["src/mail/*"],
		"@redis/*": ["src/redis/*"],
		"@logging/*": ["src/logging/*"],
		"@docker/*": ["src/docker/*"]
	}
  }
}
